<% include ./partials/header %>
<div class="resultContainer col-xs-12">
  <article class="col-xs-12">
    <h2>Unassigned</h2>
    
      <% for(var i = 0; i<data.unassigned.length; i++){%>
        <li class="recent-list">
          <%- data.unassigned[i][0] %> <br />
          - <%- data.unassigned[i][1] %>
        </li>
        <hr />
      <%}%>
    
  </article>
  <article class="col-xs-12">
    <h2>Assigned</h2>
      <% for(var i = 0; i<data.assigned.length; i++){%>
        <li class="recent-list">
          <%- data.assigned[i][0] %> <br /> 
          <a href="/<%- data.assigned[i][2] %>"><%- data.assigned[i][1] %></a><br />
          <%- data.assigned[i][2] %> <br />
          </a>
        </li>
        <hr />
      <%}%>
  </article>
</div>
<% include ./partials/footer %>
